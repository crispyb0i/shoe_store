<h1><%=@store.name%></h1>


<h3>this store has following brands in stock:</h3>
  <ul>
    <% if @brands.any? %>
      <%@brands.each do |brand|%>
        <li><a href="/brands/<%=brand.id %>">brand</li>

    <% end %>
  </ul>
    <% else %>
      <h4>There are currently no brands in this store</h4>
    <% end %>

<a href="/stores/<%= @store.id %>/edit">Edit This Store</a>
